-R
--exclude=.svn
--exclude=.git
--tag-relative=yes
--regex-PHP
--regex-PHP=/(^|^[^\/*]+[[:blank:]])class[[:blank:]]+([[:alpha:]_][[:alnum:]_]*)/\2/c/
--regex-PHP=/(^|^[^\/*]+[[:blank:]])function[[:blank:]]+&?[[:blank:]]*([[:alpha:]_][[:alnum:]_]*)/\2/f/
--regex-PHP=/interface\s+([^ {]+)/\1/c/
--regex-PHP=/const\s+([^ =]+)/\1/d/
--regex-PHP=/final\s+(public\s+|static\s+|abstract\s+|protected\s+|private\s+)function\s+\&?\s*([^ (]+)/\2/f/
--PHP-kinds=+cfd
